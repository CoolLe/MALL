<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.coolle.repository.OrderRepository">
    <select id="selectAll"
        resultType="com.coolle.entity.Order">
        select * from mall_order
    </select>

    <insert useGeneratedKeys="true" keyColumn="id" keyProperty="id"
            id="insertOrder" parameterType="com.coolle.entity.Order">
        insert into mall_order
        values
        (
        null,
        #{userId},
        #{shpId},
        #{orderId},
        #{count},
        null
        )
    </insert>
</mapper>